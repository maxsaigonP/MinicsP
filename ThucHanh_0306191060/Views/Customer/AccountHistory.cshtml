@{
}
@model IEnumerable<ThucHanh_0306191060.Areas.Admin.Models.History>

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>
                    Thời gian
                </th>
                <th>
                    Hoạt động
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Time)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Action)
                    </td>

                </tr>
            }
        </tbody>
    </table>
</div>